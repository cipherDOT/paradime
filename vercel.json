{
	"builds" : [{
		"src" : "paradime/wsgi.py",
		"use": "@vercel/python",
		"config": { "maxLambdaSize" : "15mb", "runtime" : "python3.9"},
		"api/**": {
            "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
        }
	}],
	"routes" : [
		{
			"src" : "/(.*)",
			"dest" : "paradime/wsgi.py"
		}
	]
}